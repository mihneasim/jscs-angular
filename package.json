  "author": "Glenn Jorde <glenn@tregusti.com>",
  "licenses": [{
    "type": "ISC",
    "url": "https://github.com/tregusti/jscs-angular/raw/master/LICENSE"
  }],
  "peerDependencies": {
    "jscs": "^1.5.9"
  },
  "devDependencies": {
    "chai": "^1.9.1",
    "colors": "^0.6.2",
    "jshint": "^2.5.5",
    "mocha": "^1.21.4"
